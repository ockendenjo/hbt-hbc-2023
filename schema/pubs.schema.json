{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Product",
    "description": "Pubs file",
    "type": "object",
    "properties": {
        "pubs": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number"
                    },
                    "lat": {
                        "type": "number"
                    },
                    "lon": {
                        "type": "number"
                    },
                    "name": {
                        "type": "string"
                    },
                    "address": {
                        "type": "string"
                    },
                    "visited": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "id",
                    "lat",
                    "lon",
                    "name",
                    "address"
                ]
            }
        }
    },
    "required": ["pubs"]
}
